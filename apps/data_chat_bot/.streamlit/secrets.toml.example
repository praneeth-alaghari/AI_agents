# ============================================
# Streamlit Secrets — LOCAL DEVELOPMENT ONLY
# ============================================
# Copy this file to secrets.toml and fill in your values.
# On Streamlit Cloud, paste these into the Secrets section
# in the app settings (Settings → Secrets).
#
# DO NOT commit secrets.toml to Git!

[postgres]
DB_USER = "your_pg_user"
DB_PASSWORD = "your_pg_password"
DB_HOST = "your_pg_host"
DB_PORT = "5432"
DB_NAME = "postgres"

[openai]
OPENAI_API_KEY = "sk-..."
OPENAI_MODEL = "gpt-4o"
